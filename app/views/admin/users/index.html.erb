<h1>Users</h1>
<%= will_paginate @users, renderer: FoundationPagination::Rails %>
<ul>
<% @users.each do |user| %>
  <li id="<%= user.id %>-user"> <%= link_to user.name, user %> 
  | <%= link_to "delete", admin_user_path(user), method: :delete,remote: true, data: { confirm: "You sure?" } %>
</li>
  <% end %>
</ul>  
    <%= will_paginate @users, renderer: FoundationPagination::Rails %>
